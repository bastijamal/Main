@model Doctors




<div class="container">
    <form method="post">
        <div class="row">
            <div class="mb-3">
                <label class="form-label">Title</label>
                <input asp-for="FullName" class="form-control" i>
                <span asp-validation-for="FullName" style="color:red"></span>

            </div>
            <div class="mb-3">
                <label class="form-label">SubTitle</label>
                <input asp-for="Position" class="form-control">
                <span asp-validation-for="Position" style="color:red"></span>
            </div>
            <div class="mb-3">
                <label class="form-label">ImgUrl</label>
                <input asp-for="ImgFile" class="form-control">
                <span asp-validation-for="ImgFile" style="color:red"></span>
            </div>

        </div>


        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
</div>